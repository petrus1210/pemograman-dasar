        "#deklarasi variabel set a dan b\n",
        "\n",
        "a= {1, 2, 3}\n",
        "\n",
        "b= {2, 3, 4}\n",
        "\n",
        "#operasi gabungan\n",
        "\n",
        "a.union(b)\n",
        "\n",
        "#{1, 2, 3, 4}\n",
        "\n",
        "# operasi irisan a.intersection(b)\n",
        "\n",
        "#{2,3}\n",
        "\n",
        "# operasi perbedaan\n",
        "\n",
        "a.symmetric_difference(b)\n",
        "\n",
        "#(1, 4)\n",
        "\n",
        "#lakukan print dan lihat hasilny"

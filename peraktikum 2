        "\n",
        "#False\n",
        "\n",
        "#cara mengubah nilai elemen dalam variabel dictionary\n",
        "\n",
        "propertyDetail[\"hasTerrace\"] = False\n",
        "\n",
        "propertyDetail [\"hasTerrace\"]\n",
        "\n",
        "#False\n",
